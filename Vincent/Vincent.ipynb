    "#read file from last trading day and update columns\n",
    "import pandas as pd\n",
    "def Update_Last_Day(file_name,sheet_name):\n",
    "    global pd1\n",
    "    pd1=pd.read_excel(file_name+'.xlsx',sheet_name,header=6,index_col=None)\n",
    "    pd1['C'] = pd1['D']\n",
    "    pd1['F'] = pd1['G']\n",
    "    pd1['X'] = pd1['I']\n",
    "    pd1['P'] = pd1['O']\n",
    "    pd1['S'] = pd1['R']\n",
    "    pd1['AF'] = pd1['M']\n",
    "    pd1['D'] = pd1['A']\n",
    "    pd1['R'] = pd1['U']\n",
    "    \n",
    "    pd1.set_index('K',inplace=True)"
   ]
  },
  {
   "cell_type": "code",
